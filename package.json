{
  "name": "skript-language-support",
  "displayName": "Skript Language Support",
  "description": "Syntax highlighting for skript lang.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "skript",
        "aliases": [
          "Skript",
          "skript"
        ],
        "extensions": [
          ".sk"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "skript",
        "scopeName": "source.skript",
        "path": "./syntaxes/skript.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/username/repository.git"
  }
}